#include<stdio.h>
void main()
{
int price,discount,net_price;
printf("enter the price:");
scanf("%d",&price);

discount=price*10/100;
net_price=price-discount;

discount=price*20/100;
net_price=price-discount;
if(price>1000)
printf("give discount of 20%:%d",net_price);
else
printf("give discount of 10%:%d",net_price);
}
