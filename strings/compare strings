#include<stdio.h>
#include<string.h>
void main()
{
char ch1[20],ch2[20];
int res;
printf("enter 1st string:");
gets(ch1);
printf("enter 2nd string:");
gets(ch2);

res=strcmp(ch1,ch2);  ////calculation part
if(res == 0)
puts("all are equal");
else
if(res > 0)
puts("FIRSTchr>SECONDchr");
else
puts("SECONDchr>FIRSTchr");

}
