#include<stdio.h>
void main()
{
int sal,hra,da,net_salary;

printf("Enter the marks of sal:"); //input
scanf("%d",&sal);

printf("Enter the marks of hra:");
scanf("%d",&hra);

printf("Enter the marks of da:");
scanf("%d",&da);

net_salary=hra+da+sal;

printf("net_salary is   :%6d",net_salary); //output
}

