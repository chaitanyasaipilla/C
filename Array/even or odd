
#include<stdio.h>
#include<stdlib.h>
#include<time.h>
void main()
{
    int a[10];
int i;
srand(time(0));     //random
for(i=0;i<10;i++)
{
    a[i]=rand() % 100; //1-99
    printf("%5d",a[i]);
}
for(i=0;i<10;i++)
    {
        if(a[i] % 2 == 0)
            printf("even numbers = %d\n",a[i]);
    }
    printf("\n");
    for(i=0;i<10;i++)
    {
        if(a[i] % 2 == 1 )
            printf("odd numbers = %d\n",a[i]);
    }

}
