#include<stdio.h>
#include<stdlib.h>
#include<time.h>

void main()
{
int a[5][5];
int i,j,n,found=0,count=0;

srand(time(0));
for(i=0;i<5;i++)
{
    for(j=0;j<5;j++)
    {
    a[i][j]= rand() % 50;
    printf("%5d",a[i][j]);
    }
    printf("\n");
}
  printf("enter a number:");
      scanf("%d",&n);

for(i=0; i<5 && !found;i++)
{
    for(j=0;j<5;j++)
    {
     if(n == a[i][j])
        {
        printf("found at %d, %d\n",i,j);
      found=1;
      count++;
      printf("count is %d",count);
    }
    }
}
if(!found)
    printf("not found\n");
}



