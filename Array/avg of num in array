#include<stdio.h>
#include<stdlib.h>
#include<time.h>
void main()
{
int a[10];
int i,avg,sum=0,count=0;
srand(time(0));
for(i=0;i<10;i++)
{
a[i] = rand() % 100;
printf("%5d",a[i]);
}
printf("\n");
for(i=0;i<10;i++)
{
sum+=a[i];
count++;
}
printf("total is %d\n",sum);
printf("avg is %d",sum/count);
}
